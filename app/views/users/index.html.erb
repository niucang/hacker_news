<p id="notice"><%= notice %></p>

<h1>Login</h1>

<%= form_for @user, url: login_users_path do |form| %>

  <div class="field">
    <%= form.label :username %>
    <%= form.text_field :username, id: :user_username %>
  </div>

  <div class="field">
    <%= form.label :password %>
    <%= form.text_field :password, id: :user_password %>
  </div>

  <div class="actions">
    <%= form.submit "Log in" %>
  </div>
<% end %>
